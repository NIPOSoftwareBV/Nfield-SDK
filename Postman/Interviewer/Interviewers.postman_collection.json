							"    && item.DroppedOutCount !== undefined\r",
							"    && item.RejectedCount !== undefined\r",
							"    && item.LastSyncDate !== undefined\r",
							"    && item.IsFullSynced !== undefined\r",
							"    && item.IsLastSyncSuccessful !== undefined\r",
							"    && item.IsSupervisor !== undefined;\r",
