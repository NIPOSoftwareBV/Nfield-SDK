{
	"info": {
		"_postman_id": "c0495d6f-d5d2-4bce-863c-b13bce91d41e",
		"name": "ExternalApiLogDownload",
		"description": "Manages logs collected for External API usage",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
	},
	"item": [
		{
			"name": "Prepare Download Logs",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"Status code name has string\", function () {\r",
							"    pm.response.to.have.status(\"OK\");\r",
							"});\r",
							"\r",
							"const validateProperties = (item) => {\r",
							"    return item.ActivityId !== undefined;\r",
							"}\r",
							"\r",
							"pm.test(\"Response contains AuthenticationToken\", function () {\r",
							"    const response = pm.response.json();\r",
							"    pm.expect(validateProperties(response)).to.be.true;\r",
							"    pm.environment.set(\"ActivityId\", response.ActivityId);\r",
							"});\r",
							""
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "Authorization",
						"value": "Basic {{AuthenticationToken}}",
						"type": "text"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{ \r\n    \"StartDate\" : \"2020-03-10\",\r\n    \"EndDate\": \"2020-04-10\"\r\n}\r\n\r\n\r\n\r\n\r\n",
					"options": {
						"raw": {
							"language": "json"
						}
					}
				},
				"url": {
					"raw": "{{origin}}/v1/ExternalApiLogDownload",
					"host": [
						"{{origin}}"
					],
					"path": [
						"v1",
						"ExternalApiLogDownload"
					]
				},
				"description": "Create a request for the external api logs"
			},
			"response": []
		}
	]
}