{
	"info": {
		"_postman_id": "2ad2fa89-f7e0-40a3-8f80-a620a8cbf778",
		"name": "SurveySamplingPointAddressesCount",
		"description": "Gets the number of addresses at a sampling point in a survey (Has survey type usage restrictions)",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
	},
	"item": [
		{
			"name": "Get Addresses Count",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"Status code name has string\", function () {\r",
							"    pm.response.to.have.status(\"OK\");\r",
							"});\r",
							"\r",
							""
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "GET",
				"header": [
					{
						"key": "Authorization",
						"value": "Basic {{AuthenticationToken}}",
						"type": "text"
					}
				],
				"url": {
					"raw": "{{origin}}/v1/Surveys/{{SurveyId}}/SamplingPoints/{{SamplingPointId}}/Addresses/Count",
					"host": [
						"{{origin}}"
					],
					"path": [
						"v1",
						"Surveys",
						"{{SurveyId}}",
						"SamplingPoints",
						"{{SamplingPointId}}",
						"Addresses",
						"Count"
					]
				},
				"description": "(Capi) Returns the number of addresses at the sampling point."
			},
			"response": []
		}
	]
}