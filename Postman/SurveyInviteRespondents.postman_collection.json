	"item": [
		{
			"name": "Invite Respondents",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 201\", function () {",
							"    pm.response.to.have.status(201);",
							"});",
							"",
							"pm.test(\"Response contains SurveyId\", function () {",
							"    const survey = pm.response.json();",
							"    pm.expect(survey.SurveyId.length).to.eql(36);",
							"    pm.environment.set(\"SurveyId\", survey.SurveyId);",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "Content-Type",
						"value": "application/json"
					},
					{
						"key": "X-Nfield-Domain",
						"value": "{{Domain}}"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\r\n  \"Name\": \"name\",\r\n  \"EmailColumnName\": \"email\",\r\n  \"ScheduledFor\": \"destination\",\r\n  \"InvitationTemplateId\": \"{{InvitationTemplateId}}\",\r\n  \"Filters\": \r\n  [\r\n      {\r\n           \"Name\": \"email\",\r\n           \"Op\": \"eq\",\r\n           \"Value\": \"mail@mail.com\"\r\n      }\r\n  ]\r\n}"
				},
				"url": {
					"raw": "{{origin}}/v1/Surveys/{{SurveyId}}/InviteRespondents",
					"host": [
						"{{origin}}"
					],
					"path": [
						"v1",
						"Surveys",
						"{{SurveyId}}",
						"InviteRespondents"
					]
				},
				"description": "This method invites respondents to a survey."
			},
			"response": []
		},
