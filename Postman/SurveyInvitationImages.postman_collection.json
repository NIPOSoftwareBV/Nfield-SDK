{
	"info": {
		"_postman_id": "1d514de5-3764-4032-8188-6292aacd902c",
		"name": "SurveyInvitationImages",
		"description": "A controller for querying Surveys",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
	},
	"item": [
		{
			"name": "Post Invitation Image",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 201\", function () {",
							"    pm.response.to.have.status(201);",
							"});",
							"",
							"pm.test(\"Response contains SurveyId\", function () {",
							"    const survey = pm.response.json();",
							"    pm.expect(survey.SurveyId.length).to.eql(36);",
							"    pm.environment.set(\"SurveyId\", survey.SurveyId);",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "Content-Type",
						"value": "application/json"
					},
					{
						"key": "X-Nfield-Domain",
						"value": "{{Domain}}"
					}
				],
				"url": {
					"raw": "{{origin}}/v1/Surveys/{{SurveyId}}/InvitationImages/{{FileName}}",
					"host": [
						"{{origin}}"
					],
					"path": [
						"v1",
						"Surveys",
						"{{SurveyId}}",
						"InvitationImages",
						"{{FileName}}"
					]
				},
				"description": "This method updates the ODIN script of a specific survey."
			},
			"response": []
		}
	],
	"event": [
		{
			"listen": "prerequest",
			"script": {
				"type": "text/javascript",
				"exec": [
					""
				]
			}
		},
		{
			"listen": "test",
			"script": {
				"type": "text/javascript",
				"exec": [
					""
				]
			}
		}
	]
}