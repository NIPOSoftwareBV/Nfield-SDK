{
	"info": {
		"_postman_id": "0b832152-88c0-4742-8d66-2fc2ab751f61",
		"name": "SurveyGeneralSettings",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
	},
	"item": [
		{
			"name": "Get Survey General Settings",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"Status code name has string\", function () {\r",
							"    pm.response.to.have.status(\"OK\");\r",
							"});\r",
							"\r",
							"pm.test(\"Response contains SurveyId\", function () {\r",
							"    var Surveys = pm.response.json();\r",
							"    var lastIndex = Surveys.length - 1;\r",
							"    pm.environment.set(\"SurveyId\", Surveys[lastIndex].Id);\r",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "GET",
				"header": [
					{
						"key": "Authorization",
						"value": "Basic {{AuthenticationToken}}",
						"type": "text"
					}
				],
				"url": {
					"raw": "{{origin}}/v1/SurveyGeneralSettings/{{SurveyId}}",
					"host": [
						"{{origin}}"
					],
					"path": [
						"v1",
						"SurveyGeneralSettings",
						"{{SurveyId}}"
					]
				}
			},
			"response": []
		},
		{
			"name": "Update Survey General Settings",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"Status code name has string\", function () {\r",
							"    pm.response.to.have.status(\"OK\");\r",
							"});\r",
							"\r",
							"pm.test(\"Response contains SurveyId\", function () {\r",
							"    pm.expect(pm.response.text()).to.include(\"Id\");\r",
							"    const response = pm.response.json();\r",
							"    pm.environment.set(\"SurveyId\", response.Id);\r",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "PATCH",
				"header": [
